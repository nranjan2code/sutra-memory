# Sutra AI Environment Configuration
# Copy this file to .env and fill in the values
# WARNING: Do NOT commit .env file to git

# =============================================================================
# Edition Configuration
# =============================================================================
SUTRA_EDITION=simple  # Options: simple, community, enterprise
SUTRA_VERSION=latest

# License key (required for community and enterprise editions)
SUTRA_LICENSE_KEY=
SUTRA_LICENSE_SECRET=

# =============================================================================
# Security Configuration
# =============================================================================

# JWT Secret Key (REQUIRED)
# Generate with: openssl rand -hex 32
# This MUST be set for production use
SUTRA_JWT_SECRET_KEY=

# =============================================================================
# Storage Configuration
# =============================================================================
MATRYOSHKA_DIM=768  # Embedding dimensions

# =============================================================================
# Optional: AI Service API Keys
# =============================================================================
# Only needed if using external AI services
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
