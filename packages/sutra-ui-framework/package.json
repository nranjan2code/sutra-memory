{
  "name": "@sutra/ui-framework",
  "version": "0.1.0",
  "description": "Complete UI framework for Sutra - core foundation, themes, and components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./themes": {
      "types": "./dist/themes/index.d.ts",
      "import": "./dist/themes/index.mjs",
      "require": "./dist/themes/index.js"
    },
    "./themes/holographic": {
      "types": "./dist/themes/holographic/index.d.ts",
      "import": "./dist/themes/holographic/index.mjs",
      "require": "./dist/themes/holographic/index.js"
    },
    "./themes/professional": {
      "types": "./dist/themes/professional/index.d.ts",
      "import": "./dist/themes/professional/index.mjs",
      "require": "./dist/themes/professional/index.js"
    },
    "./themes/command": {
      "types": "./dist/themes/command/index.d.ts",
      "import": "./dist/themes/command/index.mjs",
      "require": "./dist/themes/command/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/core/index.ts src/themes/index.ts src/themes/holographic/index.ts src/themes/professional/index.ts src/themes/command/index.ts src/components/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts src/core/index.ts src/themes/index.ts src/themes/holographic/index.ts src/themes/professional/index.ts src/themes/command/index.ts src/components/index.ts --format esm,cjs --dts --watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:accessibility": "jest --testPathPattern=a11y",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "sutra",
    "ui",
    "framework",
    "components",
    "theme",
    "design-system",
    "react"
  ],
  "author": "Sutra AI",
  "license": "MIT",
  "dependencies": {
    "react-window": "1.8.10",
    "d3": "7.9.0",
    "three": "0.160.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react-window": "1.8.8",
    "@types/d3": "7.4.3",
    "@types/three": "0.160.0",
    "@storybook/addon-a11y": "8.3.0",
    "@storybook/addon-essentials": "8.3.0",
    "@storybook/addon-interactions": "8.3.0",
    "@storybook/addon-links": "8.3.0",
    "@storybook/blocks": "8.3.0",
    "@storybook/react": "8.3.0",
    "@storybook/react-vite": "8.3.0",
    "@storybook/test": "8.3.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "30.0.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "9.39.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-storybook": "0.8.0",
    "jest": "29.7.0",
    "jest-axe": "9.0.0",
    "jest-environment-jsdom": "29.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "8.3.0",
    "ts-jest": "29.2.5",
    "tsup": "8.2.4",
    "typescript": "5.5.4",
    "vite": "5.4.3",
    "vitest": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nranjan2code/sutra-memory.git",
    "directory": "packages/sutra-ui-framework"
  }
}
